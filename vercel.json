{
  "version": 2,
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    },
    {
      "src": "dist/server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/$1",
      "continue": true
    },
    {
      "src": "/[^.]+",
      "dest": "dist/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 