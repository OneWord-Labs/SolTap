{
  "services": [
    {
      "type": "web",
      "name": "soltap-v2",
      "env": "node",
      "repo": "https://github.com/AKAICH00/sol-tap-v2.git",
      "plan": "free",
      "branch": "main",
      "buildCommand": "npm install --production=false && npm run build",
      "startCommand": "npm start",
      "healthCheckPath": "/api/health",
      "autoDeploy": true,
      "envVars": [
        {
          "key": "NODE_ENV",
          "value": "production"
        },
        {
          "key": "PORT",
          "value": "3001"
        }
      ]
    }
  ]
} 